FoamFile
{
    version     5.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     PBEMPPICFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1800.06;

deltaT          0.001;

writeControl    runTime;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep yes;

maxCo           0.25;
maxDeltaT       0.05;
// ************************************************************************* //
